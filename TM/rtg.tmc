%{
  long last_SSP = 0;
%}

if (last_SSP != SSP_Num) {
  ci_sendfcmd(2, "PhRTG File SSP %lu\n", SSP_Num );
  last_SSP = SSP_Num;
}
ci_sendfcmd(2, "PhRTG Datum CPU_Pct %lu %s\n", itime(), text(CPU_Pct));
ci_sendfcmd(2, "PhRTG Datum Cell_P %lu %s\n", itime(), text(Cell_P));
ci_sendfcmd(2, "PhRTG Datum Pump_Head_Ts %lu %.3lf %.3lf\n",
  itime(), convert(Pump1_Head_T), convert(Pump2_Head_T));
ci_sendfcmd(2, "PhRTG Datum Pump_RPMs %lu %u %u\n",
  itime(), RPM1, RPM2);
