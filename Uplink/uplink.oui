<package> uplink
<include> "uplink.h"
<include> msg
<follow> msg

<opts> "p:a:"
<sort>
	-p <port> serial port for uplink
	-a XX 2-digit hex instrument address

<switch>
  case 'p':
    uplink_port = optarg;
    break;
  case 'a':
    uplink_addr = optarg;
    break;

<package> msghdr_default
<var>
  char *msg_hdr_default = "Uplink";
